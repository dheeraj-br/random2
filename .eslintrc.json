{
	"env": {
		"browser": true,
		"es2021": true
	},
	"parserOptions": {
		"ecmaVersion": "latest",
		"sourceType": "module"
	},
	"ignorePatterns": [],
	"plugins": [
		"promise",
		"prettier"
	],
	"extends": [
		"airbnb-base",
		"prettier"
	],
	"rules": {
		"prettier/prettier": "error",
		"no-console": "error"
	}
}